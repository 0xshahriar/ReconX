{
  "metadata": {
    "version": "1.0.0",
    "last_updated": "2024-01-20",
    "description": "ReconX security tool definitions and installation commands"
  },
  "categories": {
    "subdomain_enum": "Subdomain enumeration tools",
    "dns": "DNS resolution and enumeration",
    "port_scanning": "Port scanners",
    "http_probe": "HTTP probing and discovery",
    "fuzzing": "Web fuzzing and brute force",
    "vulnerability_scanning": "Vulnerability scanners",
    "secret_detection": "Secret and credential finders",
    "cloud": "Cloud service enumeration",
    "osint": "Open source intelligence",
    "utilities": "General utilities"
  },
  "tools": {
    "subfinder": {
      "name": "subfinder",
      "category": "subdomain_enum",
      "description": "Fast subdomain discovery tool",
      "source": "go",
      "install_cmd": "go install -v github.com/projectdiscovery/subfinder/v2/cmd/subfinder@latest",
      "binary_path": "~/go/bin/subfinder",
      "version_check": "subfinder -version",
      "min_version": "2.6.0",
      "enabled": true,
      "config_file": "~/.config/subfinder/config.yaml",
      "rate_limit": 50,
      "timeout": 30
    },
    "amass": {
      "name": "amass",
      "category": "subdomain_enum",
      "description": "In-depth attack surface mapping and asset discovery",
      "source": "go",
      "install_cmd": "go install -v github.com/owasp-amass/amass/v4/...@master",
      "binary_path": "~/go/bin/amass",
      "version_check": "amass -version",
      "min_version": "4.2.0",
      "enabled": true,
      "config_file": "~/.config/amass/config.ini",
      "rate_limit": 10,
      "timeout": 300
    },
    "assetfinder": {
      "name": "assetfinder",
      "category": "subdomain_enum",
      "description": "Find domains and subdomains related to a given domain",
      "source": "go",
      "install_cmd": "go install -v github.com/tomnomnom/assetfinder@latest",
      "binary_path": "~/go/bin/assetfinder",
      "version_check": "assetfinder --version",
      "min_version": "0.1.1",
      "enabled": true,
      "rate_limit": 100,
      "timeout": 60
    },
    "findomain": {
      "name": "findomain",
      "category": "subdomain_enum",
      "description": "Fastest and cross-platform subdomain enumerator",
      "source": "github",
      "install_cmd": "curl -LO https://github.com/Findomain/Findomain/releases/latest/download/findomain-linux.zip && unzip findomain-linux.zip -d $PREFIX/bin/ && chmod +x $PREFIX/bin/findomain",
      "binary_path": "$PREFIX/bin/findomain",
      "version_check": "findomain --version",
      "min_version": "8.2.0",
      "enabled": true,
      "rate_limit": 50,
      "timeout": 120
    },
    "dnsx": {
      "name": "dnsx",
      "category": "dns",
      "description": "Fast and multi-purpose DNS toolkit",
      "source": "go",
      "install_cmd": "go install -v github.com/projectdiscovery/dnsx/cmd/dnsx@latest",
      "binary_path": "~/go/bin/dnsx",
      "version_check": "dnsx -version",
      "min_version": "1.1.0",
      "enabled": true,
      "rate_limit": 1000,
      "timeout": 10
    },
    "massdns": {
      "name": "massdns",
      "category": "dns",
      "description": "High-performance DNS stub resolver",
      "source": "github",
      "install_cmd": "git clone https://github.com/blechschmidt/massdns.git && cd massdns && make && cp bin/massdns $PREFIX/bin/",
      "binary_path": "$PREFIX/bin/massdns",
      "version_check": "massdns --version",
      "min_version": "1.0.0",
      "enabled": false,
      "rate_limit": 10000,
      "timeout": 300
    },
    "naabu": {
      "name": "naabu",
      "category": "port_scanning",
      "description": "Fast port scanner",
      "source": "go",
      "install_cmd": "go install -v github.com/projectdiscovery/naabu/v2/cmd/naabu@latest",
      "binary_path": "~/go/bin/naabu",
      "version_check": "naabu -version",
      "min_version": "2.1.0",
      "enabled": true,
      "config_file": "~/.config/naabu/config.yaml",
      "rate_limit": 1000,
      "timeout": 60
    },
    "nmap": {
      "name": "nmap",
      "category": "port_scanning",
      "description": "Network exploration and security auditing",
      "source": "pkg",
      "install_cmd": "pkg install nmap -y",
      "binary_path": "$PREFIX/bin/nmap",
      "version_check": "nmap --version",
      "min_version": "7.94",
      "enabled": true,
      "rate_limit": 100,
      "timeout": 300
    },
    "httpx": {
      "name": "httpx",
      "category": "http_probe",
      "description": "Fast and multi-purpose HTTP toolkit",
      "source": "go",
      "install_cmd": "go install -v github.com/projectdiscovery/httpx/cmd/httpx@latest",
      "binary_path": "~/go/bin/httpx",
      "version_check": "httpx -version",
      "min_version": "1.3.0",
      "enabled": true,
      "config_file": "~/.config/httpx/config.yaml",
      "rate_limit": 150,
      "timeout": 10
    },
    "httprobe": {
      "name": "httprobe",
      "category": "http_probe",
      "description": "Probe for working http and https servers",
      "source": "go",
      "install_cmd": "go install -v github.com/tomnomnom/httprobe@master",
      "binary_path": "~/go/bin/httprobe",
      "version_check": "httprobe --version",
      "min_version": "0.1.0",
      "enabled": false,
      "rate_limit": 100,
      "timeout": 30
    },
    "ffuf": {
      "name": "ffuf",
      "category": "fuzzing",
      "description": "Fast web fuzzer",
      "source": "go",
      "install_cmd": "go install -v github.com/ffuf/ffuf@latest",
      "binary_path": "~/go/bin/ffuf",
      "version_check": "ffuf -version",
      "min_version": "2.1.0",
      "enabled": true,
      "rate_limit": 100,
      "timeout": 60
    },
    "dirsearch": {
      "name": "dirsearch",
      "category": "fuzzing",
      "description": "Web path scanner",
      "source": "github",
      "install_cmd": "git clone https://github.com/maurosoria/dirsearch.git && cd dirsearch && pip install -e .",
      "binary_path": "~/dirsearch/dirsearch.py",
      "version_check": "python3 ~/dirsearch/dirsearch.py --version",
      "min_version": "0.4.3",
      "enabled": false,
      "rate_limit": 50,
      "timeout": 120
    },
    "nuclei": {
      "name": "nuclei",
      "category": "vulnerability_scanning",
      "description": "Fast and customizable vulnerability scanner",
      "source": "go",
      "install_cmd": "go install -v github.com/projectdiscovery/nuclei/v3/cmd/nuclei@latest",
      "binary_path": "~/go/bin/nuclei",
      "version_check": "nuclei -version",
      "min_version": "3.1.0",
      "enabled": true,
      "config_file": "~/.config/nuclei/config.yaml",
      "templates_dir": "~/.local/nuclei-templates",
      "rate_limit": 150,
      "timeout": 30,
      "update_templates_cmd": "nuclei -ut"
    },
    "gf": {
      "name": "gf",
      "category": "utilities",
      "description": "Wrapper around grep for pattern matching",
      "source": "go",
      "install_cmd": "go install -v github.com/tomnomnom/gf@latest",
      "binary_path": "~/go/bin/gf",
      "version_check": "gf --version",
      "min_version": "0.1.0",
      "enabled": true,
      "patterns_dir": "~/.gf"
    },
    "waybackurls": {
      "name": "waybackurls",
      "category": "utilities",
      "description": "Fetch URLs from Wayback Machine",
      "source": "go",
      "install_cmd": "go install -v github.com/tomnomnom/waybackurls@latest",
      "binary_path": "~/go/bin/waybackurls",
      "version_check": "waybackurls --version",
      "min_version": "0.1.0",
      "enabled": true,
      "rate_limit": 50,
      "timeout": 60
    },
    "gau": {
      "name": "gau",
      "category": "utilities",
      "description": "GetAllUrls - fetch known URLs",
      "source": "go",
      "install_cmd": "go install -v github.com/lc/gau/v2/cmd/gau@latest",
      "binary_path": "~/go/bin/gau",
      "version_check": "gau --version",
      "min_version": "2.2.0",
      "enabled": true,
      "rate_limit": 100,
      "timeout": 60
    },
    "anew": {
      "name": "anew",
      "category": "utilities",
      "description": "Append new lines to files",
      "source": "go",
      "install_cmd": "go install -v github.com/tomnomnom/anew@latest",
      "binary_path": "~/go/bin/anew",
      "version_check": "anew --version",
      "min_version": "0.1.0",
      "enabled": true
    },
    "unfurl": {
      "name": "unfurl",
      "category": "utilities",
      "description": "Pull out bits of URLs",
      "source": "go",
      "install_cmd": "go install -v github.com/tomnomnom/unfurl@latest",
      "binary_path": "~/go/bin/unfurl",
      "version_check": "unfurl --version",
      "min_version": "0.4.0",
      "enabled": true
    },
    "qsreplace": {
      "name": "qsreplace",
      "category": "utilities",
      "description": "Replace query string values",
      "source": "go",
      "install_cmd": "go install -v github.com/tomnomnom/qsreplace@latest",
      "binary_path": "~/go/bin/qsreplace",
      "version_check": "qsreplace --version",
      "min_version": "0.1.0",
      "enabled": true
    },
    "trufflehog": {
      "name": "trufflehog",
      "category": "secret_detection",
      "description": "Find credentials all over the place",
      "source": "go",
      "install_cmd": "go install -v github.com/trufflesecurity/trufflehog@latest",
      "binary_path": "~/go/bin/trufflehog",
      "version_check": "trufflehog --version",
      "min_version": "3.63.0",
      "enabled": true,
      "rate_limit": 50,
      "timeout": 300
    },
    "gitleaks": {
      "name": "gitleaks",
      "category": "secret_detection",
      "description": "Protect and discover secrets",
      "source": "go",
      "install_cmd": "go install -v github.com/gitleaks/gitleaks@latest",
      "binary_path": "~/go/bin/gitleaks",
      "version_check": "gitleaks version",
      "min_version": "8.18.0",
      "enabled": true,
      "rate_limit": 50,
      "timeout": 120
    },
    "git-dumper": {
      "name": "git-dumper",
      "category": "secret_detection",
      "description": "Dump git repository from website",
      "source": "pip",
      "install_cmd": "pip install git-dumper",
      "binary_path": "$PREFIX/bin/git-dumper",
      "version_check": "git-dumper --version",
      "min_version": "1.0.0",
      "enabled": true,
      "rate_limit": 10,
      "timeout": 300
    },
    "s3scanner": {
      "name": "s3scanner",
      "category": "cloud",
      "description": "Scan for open S3 buckets",
      "source": "pip",
      "install_cmd": "pip install s3scanner",
      "binary_path": "$PREFIX/bin/s3scanner",
      "version_check": "s3scanner --version",
      "min_version": "3.0.0",
      "enabled": true,
      "rate_limit": 10,
      "timeout": 60
    },
    "cloud_enum": {
      "name": "cloud_enum",
      "category": "cloud",
      "description": "Multi-cloud OSINT tool",
      "source": "github",
      "install_cmd": "git clone https://github.com/initstring/cloud_enum.git",
      "binary_path": "~/cloud_enum/cloud_enum.py",
      "version_check": "python3 ~/cloud_enum/cloud_enum.py --help",
      "min_version": "1.0.0",
      "enabled": false,
      "rate_limit": 5,
      "timeout": 300
    },
    "theHarvester": {
      "name": "theHarvester",
      "category": "osint",
      "description": "Email harvesting and OSINT",
      "source": "github",
      "install_cmd": "git clone https://github.com/laramies/theHarvester.git && cd theHarvester && pip install -r requirements.txt",
      "binary_path": "~/theHarvester/theHarvester.py",
      "version_check": "python3 ~/theHarvester/theHarvester.py --help",
      "min_version": "4.4.0",
      "enabled": false,
      "rate_limit": 10,
      "timeout": 300
    },
    "asnmap": {
      "name": "asnmap",
      "category": "utilities",
      "description": "ASN lookup and enumeration",
      "source": "go",
      "install_cmd": "go install -v github.com/projectdiscovery/asnmap/cmd/asnmap@latest",
      "binary_path": "~/go/bin/asnmap",
      "version_check": "asnmap -version",
      "min_version": "1.0.0",
      "enabled": true,
      "rate_limit": 50,
      "timeout": 30
    },
    "tlsx": {
      "name": "tlsx",
      "category": "utilities",
      "description": "TLS certificate scanner",
      "source": "go",
      "install_cmd": "go install -v github.com/projectdiscovery/tlsx/cmd/tlsx@latest",
      "binary_path": "~/go/bin/tlsx",
      "version_check": "tlsx -version",
      "min_version": "1.1.0",
      "enabled": true,
      "rate_limit": 100,
      "timeout": 10
    }
  },
  "wordlist_sources": {
    "seclists": {
      "url": "https://github.com/danielmiessler/SecLists.git",
      "local_path": "~/wordlists/SecLists",
      "categories": ["Discovery", "DNS", "Web-Content", "Fuzzing"]
    },
    "payloads_all_things": {
      "url": "https://github.com/swisskyrepo/PayloadsAllTheThings.git",
      "local_path": "~/wordlists/PayloadsAllTheThings",
      "categories": ["Command Injection", "LDAP Injection", "NoSQL Injection", "SQL Injection", "XSS Injection"]
    }
  }
}
